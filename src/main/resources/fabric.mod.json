{
	"schemaVersion": 1,
	"id": "itemphobia",
	"version": "${version}",
	"name": "Itemphobia",
	"description": "A mod that allows you to blacklist items that you do not want to pick up from the ground",
	"authors": [
		"smit4k"
	],
	"contact": {
		"homepage": "https://github.com/smit4k/Itemphobia",
		"sources": "https://github.com/smit4k/Itemphobia"
	},
	"license": "CC0-1.0",
	"icon": "assets/itemphobia/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"codes.smit.Itemphobia"
		],
		"client": [
			"codes.smit.ItemphobiaClient"
		]
	},
	"mixins": [
		"itemphobia.mixins.json",
		{
			"config": "itemphobia.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": ">=1.21.0 <1.22.0",
		"java": ">=21",
		"fabric-api": "*"
	}
}